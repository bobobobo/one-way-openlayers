{
  "name": "one-way-openlayers",
  "version": "0.0.1",
  "description": "One way dataflow wrapper for OpenLayers maps",
  "main": "dist/index.js",
  "scripts": {
    "test": "./node_modules/karma/bin/karma start",
    "start": "webpack-dev-server --watch --inline --content-base src/",
    "flow": "./node_modules/flow-bin/cli.js",
    "deploy": "babel lib --presets babel-preset-es2015 --out-dir dist",
    "postinstall": "npm run deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itjope/one-way-openlayers.git"
  },
  "keywords": [
    "map",
    "ol3",
    "openlayers"
  ],
  "author": "Jonas Petersson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/itjope/one-way-openlayers/issues"
  },
  "homepage": "https://github.com/itjope/one-way-openlayers#readme",
  "dependencies": {
    "diff-json": "^0.1.11",
    "lodash": "^4.17.4",
    "ol": "^3.21.0-beta.16",
    "proj4": "^2.3.17"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "flow-bin": "^0.38.0",
    "html-webpack-plugin": "^2.28.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.4.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^2.0.2",
    "webpack": "^2.2.0"
  }
}
